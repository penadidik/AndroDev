<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="di2k.csi.androloginv15k">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/andrologin"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/andrologin"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name="di2k.csi.androloginv15k.main.ProfileActivity"/>
        <activity android:name="di2k.csi.androloginv15k.main.SignInActivity" />
        <activity android:name="di2k.csi.androloginv15k.main.SignUpActivity" />
        <activity android:name="di2k.csi.androloginv15k.main.MainActivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>